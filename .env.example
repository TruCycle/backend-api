NODE_ENV=development
PORT=3000

# Toggle database integration (TypeORM)
# Set to true when you have a Postgres instance available (local or hosted)
ENABLE_DB=true

# Database
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASS=postgres
DB_NAME=trucycle
DB_SSL=false
DB_LOGGING=false

# Dev only: allow TypeORM to sync schema for quick start
TYPEORM_SYNC=true

# CORS
CORS_ORIGINS=*
