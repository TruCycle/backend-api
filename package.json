{
  "name": "trucycle-backend-api",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --respawn --transpileOnly src/main.ts",
    "start:debug": "node --inspect-brk -r ts-node/register src/main.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "migration:run": "ts-node -r dotenv/config -r tsconfig-paths/register ./src/scripts/run-migrations.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/swagger": "^7.4.2",
    "swagger-ui-express": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "@nestjs/typeorm": "^10.0.0",
    "typeorm": "^0.3.20",
    "pg": "^8.11.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  }
}
